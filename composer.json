{
    "name": "aherzog/starconnect",
    "description": "Starconnect ",
    "keywords": [
        "aherzog",
        "starconnect"
    ],
    "homepage": "https://github.com/aherzog/starconnect",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alexander Herzog",
            "email": "alexander@alexander-herzog.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "AHerzog\\Starconnect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AHerzog\\Starconnect\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AHerzog\\Starconnect\\StarconnectServiceProvider"
            ],
            "aliases": {
                "Starconnect": "AHerzog\\Starconnect\\StarconnectFacade"
            }
        }
    }
}
